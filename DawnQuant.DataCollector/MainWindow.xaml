<dx:DXTabbedWindow
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
    xmlns:AShareViews="clr-namespace:DawnQuant.DataCollector.Views.AShare"
    xmlns:OtherViews="clr-namespace:DawnQuant.DataCollector.Views.Other"
    xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" 
    x:Class="DawnQuant.DataCollector.MainWindow"
    Title="DawnQuant.DataCollector" ShowTitle="False" ShowIcon="False"  
    Icon="/Assets/Icons/app.ico" Height="768" Width="1200" WindowState="Maximized" >

    <dx:DXTabControl TabContentCacheMode="CacheAllTabs" Padding="0" Margin="0">
        <dx:DXTabItem FontSize="15" Header="沪深股市" >
            <AShareViews:AShareMainView></AShareViews:AShareMainView>
        </dx:DXTabItem>
        <dx:DXTabItem FontSize="15" Header="其他" >
            <OtherViews:DynamicDnsView></OtherViews:DynamicDnsView>
        </dx:DXTabItem>
    </dx:DXTabControl>

</dx:DXTabbedWindow>
